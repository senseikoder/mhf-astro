---
export const prerender = false;
import EventPost from "../../layouts/EventPost.astro";

const { slug } = Astro.params;

let res = await fetch(
   `https://myhealthfair.com/wp-json/tribe/events/v1/events/by-slug/${slug}`,
);
let evento = await res.json();

---

<EventPost {...evento}>
   <Fragment set:html={evento.description} />
</EventPost>
